<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.simple.server.domain">
	
	<class name="BusSqlParam" table="[bus sql param]">
		<composite-id> 
			<key-many-to-one name="tag" class="BusSqlTemplate" column="[tag]"/> 
			<key-property name="paramName" column="[param]"/> 
		</composite-id>
	
		<property name="paramDesc" column="[desc]" />
	</class>
</hibernate-mapping>
