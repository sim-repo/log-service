<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.simple.server.domain">

	<class name="BusSqlTemplate" table="[bus sql template]">
		<id name="tag" column="[tag]"/>										
		<property name="sqlTemplate" column="[sql_template]" />		

    	<set name="params" table="[bus sql param]">
	        <key column="[tag]"/>
	        <many-to-many column="[id]"
	            unique="true"
	            class="BusSqlParam"/>
    	</set>
	</class>
</hibernate-mapping>
